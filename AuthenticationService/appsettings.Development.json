{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "urls": "http://localhost:8081",
  "Token": {
    "Secret": "abcdefghijklmn1234567890",
    "Issuer": "webapi.cn",
    "Audience": "WebApi",
    "AccessExpiration": 30,
    "RefreshExpiration": 60
  },
  "ConnectionStrings": {
    "Mysql": "server=localhost; port=3306; database=user; user=root; password=123456;  Persist Security Info=false; Connect Timeout=300"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Console",
        "Args": {}
      }
    ]
  }
}
